<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <EditText
        android:id="@+id/et_getFocus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:inputType="none" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_38dp"
        android:layout_marginLeft="@dimen/_6dp"
        android:layout_marginRight="@dimen/_6dp"
        android:layout_marginTop="@dimen/_10dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="2"
            android:gravity="center"
            android:text="移出容器"
            android:textColor="#000000"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/back_style_blue"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_positionScan"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:background="@drawable/btn_scan_selector" />

            <EditText
                android:id="@+id/et_positionCode"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/transparent"
                android:hint="扫描容器条码"
                android:inputType="none"
                android:paddingLeft="@dimen/_2dp"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sp" />

            <Button
                android:id="@+id/btn_positionSel"
                android:layout_width="@dimen/_42dp"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_1dp"
                android:background="@drawable/btn_blue_selector"
                android:text="选择"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10dp"
        android:layout_marginRight="@dimen/_10dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_stockName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="仓库："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_stockAreaName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="库区："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_storageRackName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="货架："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_stockPosName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="库位："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_containerName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="容器："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_38dp"
        android:layout_marginLeft="@dimen/_6dp"
        android:layout_marginRight="@dimen/_6dp"
        android:layout_marginTop="@dimen/_16dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="2"
            android:gravity="center"
            android:text="移入位置"
            android:textColor="#000000"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/back_style_blue"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_positionScan2"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:background="@drawable/btn_scan_selector" />

            <EditText
                android:id="@+id/et_positionCode2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/transparent"
                android:hint="扫描位置条码"
                android:inputType="none"
                android:paddingLeft="@dimen/_2dp"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sp" />

            <Button
                android:id="@+id/btn_positionSel2"
                android:layout_width="@dimen/_42dp"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/_1dp"
                android:background="@drawable/btn_blue_selector"
                android:text="选择"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_12dp"
        android:layout_marginRight="@dimen/_6dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_stockName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="仓库："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_stockAreaName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="库区："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_storageRackName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="货架："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

        <TextView
            android:id="@+id/tv_stockPosName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="库位："
            android:textColor="@color/c_666666"
            android:textSize="@dimen/_12sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_38dp"
        android:gravity="center"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_clone"
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:layout_weight="1"
            android:background="@drawable/btn_gray2_selector"
            android:gravity="center"
            android:text="重置"
            android:textColor="@color/c_666666"
            android:textSize="16sp" />

        <Button
            android:id="@+id/btn_save"
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:layout_weight="2"
            android:background="@drawable/btn_blue2_selector"
            android:gravity="center"
            android:text="确认移动"
            android:textColor="@color/white"
            android:textSize="16sp" />

    </LinearLayout>


</LinearLayout>

